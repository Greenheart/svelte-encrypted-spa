        "autoprefixer": "^10.4.14",
        "dotenv": "^16.3.1",
        "fs-extra": "^11.1.1",
        "pagecrypt": "^6.1.1",
        "postcss": "^8.4.27",
        "postcss-load-config": "^4.0.1",
        "prettier": "^3.0.1",
        "prettier-plugin-svelte": "^3.0.3",
        "svelte": "^4.1.2",
        "svelte-check": "^3.4.6",
        "tailwindcss": "^3.3.3",
        "typescript": "^5.1.6",
        "vite": "^4.4.9",
        "vite-plugin-singlefile": "^0.13.5"
    },
    "prettier": {
        "semi": false,
        "tabWidth": 4,
        "singleQuote": true,
