        "autoprefixer": "^10.4.13",
        "dotenv": "^16.0.3",
        "fs-extra": "^11.1.0",
        "pagecrypt": "^6.0.1",
        "postcss": "^8.4.21",
        "postcss-load-config": "^4.0.1",
        "prettier": "^2.8.4",
        "prettier-plugin-svelte": "^2.9.0",
        "svelte": "^3.55.1",
        "svelte-check": "^3.0.4",
        "tailwindcss": "^3.2.7",
        "typescript": "^4.9.5",
        "vite": "^4.1.4",
        "vite-plugin-singlefile": "^0.13.3"
    },
    "prettier": {
        "semi": false,
        "tabWidth": 4,
        "singleQuote": true,
