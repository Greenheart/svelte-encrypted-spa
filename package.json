{
    "name": "svelte-encrypted-spa",
    "author": "Samuel Plumppu",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "vite build && node scripts/postbuild.js",
        "preview": "vite preview",
        "check": "svelte-check --tsconfig ./tsconfig.json"
    },
    "dependencies": {
        "svelte-pathfinder": "^4.7.1"
    },
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^2.4.4",
