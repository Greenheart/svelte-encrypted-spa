        "dotenv": "^16.0.3",
        "fs-extra": "^10.1.0",
        "pagecrypt": "^5.4.0",
        "postcss": "^8.4.17",
        "postcss-load-config": "^4.0.1",
        "prettier": "^2.7.1",
        "prettier-plugin-svelte": "^2.7.1",
        "svelte": "^3.50.1",
        "svelte-check": "^2.9.1",
        "svelte-preprocess": "^4.10.7",
